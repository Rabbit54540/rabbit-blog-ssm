<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>

    <!--配置数据源，交给Spring去做-->
    <typeAliases>
        <typeAlias alias="User" type="com.rabbit.pojo.User"/>
        <typeAlias alias="Blog" type="com.rabbit.pojo.Blog"/>
        <typeAlias alias="Like" type="com.rabbit.pojo.Like"/>
        <typeAlias alias="Image" type="com.rabbit.pojo.Image"/>
    </typeAliases>


</configuration>
